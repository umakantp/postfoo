{
  "compilerOptions": {
    "lib": ["es6", "dom", "dom.iterable", "esnext"],
    "target": "es5",
    "skipLibCheck": true,
    "strict": true,
    "noEmit": true,
    "esModuleInterop": true,
    "module": "esnext",
    "sourceMap": true,
    "allowJs": false,
    "jsx": "preserve",
    "moduleResolution": "bundler",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "incremental": true,
    "plugins": [
      {
        "name": "next"
      }
    ],
    "outDir": "build/dist",
    "rootDir": ".",
    "baseUrl": ".",
    "paths": {
      "src/*": ["src/*"],
    }
  },
  "include": ["next-env.d.ts", "**/*.ts", "**/*.tsx", ".next/types/**/*.ts", "next.config.mjs"],
  "exclude": [
    "node_modules",
    "**/build",
    "**/dist",
    "**/public"
  ]
}
